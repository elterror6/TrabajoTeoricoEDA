package Tree;

/**
 * Esta interfaz representa un nodo de una estructura de Ã¡rbol.
 *
 * @param <V> El tipo del elemento que va a contener el nodo.
 */
public interface Node <V> {
	
	/**
	 * Gets the id.
	 *
	 * @return the id
	 */
	public abstract int getID ();
	
	/**
	 * Gets the element.
	 *
	 * @return the element
	 */
	public abstract V getElement ();
}
